# Example environment variables for Meltano pipeline
# Copy this file to .env and fill in your actual credentials

# MySQL Database Credentials
TAP_MYSQL_PASSWORD='your_mysql_password_here'

# Google Cloud BigQuery Credentials
# Obtain these from your Google Cloud service account JSON file
GOOGLE_CLOUD_PROJECT_ID='your_project_id'
GOOGLE_CLOUD_PRIVATE_KEY_ID='your_private_key_id'
GOOGLE_CLOUD_PRIVATE_KEY='your_private_key_with_newlines'
GOOGLE_CLOUD_CLIENT_EMAIL='your_service_account_email'
GOOGLE_CLOUD_CLIENT_ID='your_client_id'
GOOGLE_CLOUD_AUTH_URI='https://accounts.google.com/o/oauth2/auth'
GOOGLE_CLOUD_TOKEN_URI='https://oauth2.googleapis.com/token'
GOOGLE_CLOUD_AUTH_PROVIDER_X509_CERT_URL='https://www.googleapis.com/oauth2/v1/certs'
GOOGLE_CLOUD_CLIENT_X509_CERT_URL='your_client_x509_cert_url'
GOOGLE_CLOUD_UNIVERSE_DOMAIN='googleapis.com'

# BigQuery Configuration
TARGET_BIGQUERY_PROJECT='your_bigquery_project_id'
TARGET_BIGQUERY_DATASET='your_dataset_name'
